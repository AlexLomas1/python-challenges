def DuplicatesRemoval(Original):
    NewList = []
    for Count in range(0,len(Original)):
        #Checks if current element of the original list is a duplicate
        if Original[Count] not in NewList:
            #Only if the current element is not a duplicate will it be added to NewList
            NewList.append(Original[Count])
    #Returns the array without any duplicates
    return NewList

#Takes the users input as an array, with each element in the input seperated by a space
OriginalList = [item for item in input("Enter elements of list: ").split()]
#Prints the array without any duplicates
print(DuplicatesRemoval(OriginalList))